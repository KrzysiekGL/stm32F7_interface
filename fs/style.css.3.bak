body {
  background-color: #002020;
}

#container {
  width: 900px;
  min-height: 350px;
  margin-top: 50px;
  margin-left: auto;
  margin-right: auto;
  color: #8AB4BF;
  text-align: center;
  border: 3px solid #FF7AC6;
  border-radius: 25px;
  background-color: #003333;
}

h1 {
  color: #D06771;
  font-size: 48px;
}

h2 {
  color: #D06269;
  font-size: 34px;
}

#setLEDsDiv {
  width: 176px;
  margin-left: auto;
  margin-right: auto;
  border: 2px solid #FF7AC6;
  border-radius: 25px;
  text-align: center;
}

.LEDBtn {
  float: left;
  display: inline-block;
  background-color: #000000;
  width: 20px;
  height: 20px;
  margin: 10px;
  border: 2px solid #111111;
  border-radius: 15px;
}

.LEDBtn:hover {
  cursor: pointer;
}

#checkInputsDiv {
  width: 250px;
  margin-left: auto;
  margin-right: auto;
  margin-top: 25px;
  margin-bottom: 50px;
  border: 2px solid #FF7AC6;
  border-radius: 25px;
}

.InputBtn {
  margin-top: 7px;
  margin-bottom: 7px;
  border: 1px solid #111111;
  border-radius: 5px;
  width: 100px;
  margin-left: auto;
  margin-right: auto;
  background-color: #000000;
}
