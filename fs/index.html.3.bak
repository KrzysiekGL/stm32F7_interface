<!DOCTYPE html>
<html lang="pl">
  <head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <link rel="stylesheet" href="style.css">

    <script src="controller.js"></script>

  </head>

  <body>

    <div id="container">
      <div id="header">
      <h1>STM32F7</h1>
      <h2>Interface</h2>
    </div>

    <div id="setLEDsDiv">
      Set LED state:
      <div class="LEDBtn" id="LEDBtn1" onclick="setLEDState(1);">1</div>
      <div class="LEDBtn" id="LEDBtn2" onclick="setLEDState(2);">2</div>
      <div class="LEDBtn" id="LEDBtn3" onclick="setLEDState(3);">3</div>
      <div class="LEDBtn" id="LEDBtn4" onclick="setLEDState(4);">4</div>
      <div style="clear: both;"></div>
    </div>

    <div id="checkInputsDiv">
      Check Inputs status:
      <div class="InputBtn" id="InputBtn1">Input 1</div>
      <div class="InputBtn" id="InputBtn2">Input 2</div>
      <div class="InputBtn" id="InputBtn3">Input 3</div>
      <div class="InputBtn" id="InputBtn4">Input 4</div>
    </div>

  </div>

  </body>
</html>
